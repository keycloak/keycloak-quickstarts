                        }
                    },
                    {
                        "name": "Default Resource Permission",
                        "description": "A permission that applies to the default resource",
                        "type": "resource",
                        "logic": "POSITIVE",
                        "decisionStrategy": "UNANIMOUS",
                        "config": {
                            "resources": "[\"Default Resource\"]",
